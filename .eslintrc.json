{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "es2023": true,
    "mongo": true
  },
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "airbnb", "airbnb/hooks", "prettier"],
  "plugins": [],
  "rules": {
    "no-use-before-define": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-undef": "off",
    "no-plusplus": "off"
  }
}
